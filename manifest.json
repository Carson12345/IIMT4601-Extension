{
    "manifest_version": 2,
    "name": "Reddit Verifier",
    "description": "Rate reddit users!",
    "version": "2.0",
    "content_scripts": [
        {
            "matches": [
                "https://www.reddit.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "page_action": {
        "default_icon": "reddit_logo.png",
        "default_popup": "popup.html",
        "default_title": "My custom google page!"
    }
}